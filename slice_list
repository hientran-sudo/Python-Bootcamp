piano_keys = ["a","b","c","d","e","f","g"]
piano_tuple = ("do", "re", "mi", "fa", "son", "la", "ti")

print(piano_keys[2:5]) #print ['c', 'd', 'e']
print(piano_keys[2:]) #print ['c', 'd', 'e', 'f', 'g']
print(piano_keys[:5]) #print ['a', 'b', 'c', 'd', 'e']
print(piano_keys[2:5:2]) #print ['c', 'e']
print(piano_keys[::-1]) #print ['g', 'f', 'e', 'd', 'c', 'b', 'a']

print(piano_tuple[2])
